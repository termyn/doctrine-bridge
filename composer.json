{
    "name": "termyn/doctrine-bridge",
    "description": "Provides integration for Doctrine with Termyn components",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edo Barnas",
            "email": "iam@edobarnas.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Termyn\\Bridge\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Termyn\\Bridge\\Doctrine\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "doctrine/dbal": "^3.4",
        "phpstan/phpstan": "1.9.*",
        "phpstan/phpstan-phpunit": "1.3.*",
        "phpunit/phpunit": "9.5.*",
        "symplify/easy-coding-standard": "11.1.*",
        "termyn/date-time": "dev-main",
        "termyn/id": "dev-main"
    }
}
